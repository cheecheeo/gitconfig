[alias]
  b = branch -avv
  lp = log -p
  cb = checkout -b
  co = checkout
  d = diff
  ds = diff --stat
  lpp = log --graph --pretty=format:'%C(yellow)%h%Creset %Cgreen%cr%Creset %C(blue)%an%Creset%C(red)%d%Creset %s' --branches
  r = remote -v
  rem = rebase master
  cm = checkout master
[branch]
  autosetuprebase = always
